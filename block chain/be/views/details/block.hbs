<br />
<h1>Block Details</h1>
<div class="container-fluid">
  <div class="row">
    <div class="span10" style="width: 100%">
      <table class="table table-striped table-condensed">
        <thead>
          <tr>
            <th>Column</th>
            <th>Detail</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>TimeStamp</td>
            <td>{{content.timestamp}}</td>
          </tr>
          <tr>
            <td>Hash</td>
            <td>{{content.hash}}</td>
          </tr>
          <tr>
            <td>Previous Hash</td>
            <td>{{content.lastHash}}</td>
          </tr>
          <tr>
            <td>Miner</td>
            <td>{{content.miner}}</td>
          </tr>
          <tr>
            <td>Difficulty</td>
            <td>{{content.difficulty}}</td>
          </tr>
          <tr>
            <td>Nonce</td>
            <td>{{content.nonce}}</td>
          </tr>
          <tr>
            <td>Data</td>
            <td>
              <pre>
                <code>
                  {{content.data}}
                </code></pre>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>